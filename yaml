apiVersion: apps/v1
kind: DaemonSet
metadata:
  name: nginx-ds
  namespace: default
  labels:
    name: nginx-ds
spec:
  selector:
    matchLabels:
      name: nginx-ds
  template:
    spec:
      containers:
        - name: nginx
          image: nginx:1.17.8
          imagePullPolicy: IfNotPresent
          ports:
          - containerPort: 80
        
